<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--==============-->
    <!--Navigation Bar-->
    <!--==============-->
    <style name="Widget.Mathack.BottomNavigationView" parent="Widget.Material3.BottomNavigationView">
        <item name="materialThemeOverlay">@style/ThemeOverlay.Mathack.BottomNavigationView</item>
    </style>

    <style name="ThemeOverlay.Mathack.BottomNavigationView" parent="">
        <item name="background">@color/mathack_navigationBar</item>
        <item name="colorOnSurfaceVariant">@color/mathack_onSurfaceVariant</item>
    </style>


    <!--=======-->
    <!--Toolbar-->
    <!--=======-->
    <style name="Widget.Mathack.Toolbar" parent="Widget.Material3.Toolbar">
        <item name="materialThemeOverlay">@style/ThemeOverlay.Mathack.Toolbar</item>
    </style>

    <style name="ThemeOverlay.Mathack.Toolbar" parent="">
        <item name="android:background">@color/mathack_androidNavigation</item>
        <item name="iconTint">@color/mathack_onSurfaceVariant</item>
    </style>
</resources>